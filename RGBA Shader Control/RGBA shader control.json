{
    "params": [
        {
            "name": "Shader File name (the file should be in assets/shaders/you_shader.frag/vert)",
            "type": "String",
            "defaultValue": "garage-shadow"
        },
        {
            "name": "Character to aply shader to",
            "type": "StrumLine",
            "defaultValue": ""
        },{
            "name": "Character number (look at the character's ordening in the chart editor) left to right",
            "type": "Float(0, 10, 1, 2)",
            "defaultValue": "0"
        },
               {
            "name": "Red",
            "type": "Float(0, 1, 0.01, 2)",
            "defaultValue": "1"
        },
                {
            "name": "Green",
            "type": "Float(0, 1, 0.01, 2)",
            "defaultValue": "0"
        },
                {
            "name": "Blue",
            "type": "Float(0, 1, 0.01, 2)",
            "defaultValue": "0"
        },
                {
            "name": "Alpha",
            "type": "Float(0, 1, 0.01, 2)",
            "defaultValue": "1"
        },
                {
            "name": "How big the shader light is",
            "type": "Float(0, 100, 0.1, 2)",
            "defaultValue": "25"
        },
                {
            "name": "How dark is the shadow 0 = fully opaque, 1 = fully transparent",
            "type": "Float(0, 1, 0.01, 2)",
            "defaultValue": "0.5"
        },
         {
            "name": "Flpped X? true = left, false = right",
            "type": "Bool",
            "defaultValue": "false"
        },
        {
            "name": "Remove shader?",
            "type": "Bool",
            "defaultValue": "false"
        }
    ]
}